{
    "name": "aws-network",
    "x-type": "hlfv1",
    "version": "1.0.0",
    "client": {
        "organization": "org1MSP",
        "credentialStore": {
            "path": "crypto/kvs"
        },
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300",
                    "eventHub": "300",
                    "eventReg": "300"
